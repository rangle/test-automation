{
  "name": "todos-cypress",
  "version": "1.0.0",
  "description": "Demonstrates how to implement basic e2e testing in Cypress against  a sample application",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "cypress": "^3.1.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/cypress open",
    "test:headless": "./node_modules/.bin/cypress run --spec 'cypress/integration/todosApp/*' --record --key e16ffb96-272f-4932-b1dc-a72f00499304 --parallel",
    "test:headless:all": "./node_modules/.bin/cypress run",
    "cy:verify": "./node_modules/.bin/cypress verify"
  }
}
