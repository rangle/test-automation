{
  "name": "webdriverio",
  "description": "A Node.js bindings implementation for the W3C WebDriver protocol",
  "version": "4.12.0",
  "homepage": "http://webdriver.io",
  "author": "Camilo Tapia <camilo.tapia@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/webdriverio/webdriverio.git"
  },
  "bugs": {
    "url": "https://github.com/webdriverio/webdriverio/issues"
  },
  "license": "MIT",
  "main": "./build/index.js",
  "engines": {
    "node": ">= 4.8.5"
  },
  "eslintConfig": {
        "env": {
            "browser": true
        }
    },
  "scripts": {
    "build": "run-s clean compile copy",
    "clean": "run-p clean:*",
    "clean:build": "rm -fr build",
    "clean:coverage": "rm -fr coverage",
    "compile": "run-p compile:*",
    "compile:index": "mkdir -p build && babel index.js --out-file build/index.js",
    "compile:lib": "babel lib/ -d build/lib",
    "copy": "run-p copy:*",
    "copy:packagejson": "cp package.json build",
    "copy:ejstemplate": "cp lib/helpers/wdio.conf.ejs build/lib/helpers/wdio.conf.ejs",
    "eslint": "eslint ./lib test/",
    "release": "np patch",
    "release:patch": "np patch",
    "release:minor": "np minor",
    "release:major": "np major",
    "test": "run-s eslint test:unit",
    "test:ci": "run-s eslint build test:$_ENV",
    "test:desktop": "mocha test/setup.js test/spec/*.js test/spec/desktop/*.js",
    "test:functional": "mocha test/setup.js test/spec/functional/**/*.js",
    "test:multibrowser": "mocha test/setup.js test/spec/multibrowser/**/*.js",
    "test:wdio": "mocha test/setup-unit.js test/spec/wdio/*.js",
    "postversion": "run-s build",
    "prepare": "npm prune",
    "watch": "npm run compile:lib -- --watch",
    "tidy": "prettier-eslint \"**/*.js\""
  },
  "dependencies": {
    "archiver": "~2.1.0",
    "babel-runtime": "^6.26.0",
    "css-parse": "~2.0.0",
    "css-value": "~0.0.1",
    "deepmerge": "~2.0.1",
    "ejs": "~2.5.6",
    "gaze": "~1.1.2",
    "glob": "~7.1.1",
    "inquirer": "~3.3.0",
    "json-stringify-safe": "~5.0.1",
    "mkdirp": "~0.5.1",
    "npm-install-package": "~2.1.0",
    "optimist": "~0.6.1",
    "q": "~1.5.0",
    "request": "~2.83.0",
    "rgb2hex": "~0.1.0",
    "safe-buffer": "~5.1.1",
    "supports-color": "~5.0.0",
    "url": "~0.11.0",
    "wgxpath": "~1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.2",
    "chai-string": "^1.3.0",
    "chai-things": "^0.2.0",
    "coveralls": "^3.0.0",
    "gulp": "^3.9.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.5",
    "mocha": "^3.5.3",
    "mock-require": "^2.0.2",
    "nock": "^9.2.3",
    "node-static": "^0.7.9",
    "np": "^2.16.0",
    "npm-run-all": "^4.0.2",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "sauce-connect-launcher": "^1.2.1",
    "saucelabs": "^1.4.0",
    "selenium-standalone": "^6.13.0",
    "snyk": "^1.70.0",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12",
    "yo": "^2.0.1"
  },
  "tags": [
    "test",
    "automation",
    "browser",
    "javascript",
    "node"
  ],
  "keywords": [
    "mocha",
    "chai",
    "vows",
    "jasmine",
    "assert"
  ]
}
